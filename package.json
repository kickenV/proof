{
  "name": "chefsplan-mvp",
  "version": "1.0.0",
  "description": "ChefsPlan MVP - Decentralized gig platform for chefs",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:all": "node scripts/test-runner.js",
    "test:coverage": "node scripts/test-runner.js --coverage",
    "test:quick": "node scripts/test-runner.js --quick",
    "test:reputation": "hardhat test test/Reputation.test.js",
    "test:escrow": "hardhat test test/Escrow.test.js",
    "test:shiftmanager": "hardhat test test/ShiftManager.test.js",
    "test:integration": "hardhat test test/integration/",
    "test:deployment": "hardhat test test/deployment.test.js",
    "deploy": "hardhat run scripts/deploy.js",
    "deploy:testnet": "hardhat run scripts/deploy.js --network zkSyncTestnet",
    "verify": "hardhat run scripts/verify.js --network zkSyncTestnet",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "test:frontend": "jest",
    "agents:start": "node agents/startup.js"
